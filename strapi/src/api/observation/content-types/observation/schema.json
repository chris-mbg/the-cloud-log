{
  "kind": "collectionType",
  "collectionName": "observations",
  "info": {
    "singularName": "observation",
    "pluralName": "observations",
    "displayName": "Observation"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
